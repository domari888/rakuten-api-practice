<table>
  <thead>
    <tr>
      <th>画像</th>
      <th>商品名</th>
      <th>ジャンル</th>
    </tr>
  </thead>
  <tbody>
    <% if @items.present? %>
      <% @items.each do |item| %>
        <tr>
          <td><%= image_tag item['smallImageUrl'] %></td>
          <td><%= link_to item['productName'], "#{item['affiliateUrl']}" %></td>
          <td><%= item['genreName'] %></td>
        </tr>
      <% end %>
    <% end %>
  </tbody>
</table>